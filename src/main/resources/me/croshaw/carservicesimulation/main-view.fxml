<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="739.0" prefWidth="1163.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.croshaw.carservicesimulation.MainController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <ScrollPane hbarPolicy="NEVER" hmax="0.0" prefHeight="659.0" prefWidth="1123.0">
         <content>
            <FlowPane fx:id="simulationPanel" maxWidth="1112.0" prefHeight="503.0" prefWidth="1112.0" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
         <children>
            <Button fx:id="startNewSimulation" mnemonicParsing="false" onAction="#onStartNewSimulationButtonClick" prefHeight="24.0" prefWidth="204.0" text="Запустисть новую симуляцию" />
            <Button mnemonicParsing="false" onAction="#onLoadButtonClick" prefHeight="22.0" prefWidth="180.0" text="Загрузить с файла" />
         </children>
      </HBox>
   </children>
</VBox>
